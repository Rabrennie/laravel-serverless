{
    "name": "rabrennie/laravel-serverless",
    "description": "Serverless laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rab",
            "email": "rab@rabrennie.com"
        }
    ],
    "require": {
        "symfony/yaml": "^4.3",
        "illuminate/support": "^5.8",
        "illuminate/console": "^5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "orchestra/testbench": "^3.8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "LaravelServerless\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelServerless\\Test\\": "tests"
        }
    }
}
